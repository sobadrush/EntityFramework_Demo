@* @model System.Collections.Generic.List<CodeFirst_Demo.Models.UserModel> *@
@model IEnumerable<CodeFirst_Demo.Models.UserModel>

@{
    ViewBag.Title = "User";
    Layout = "_Layout";
}

@{
    Model.ToList().ForEach(Console.WriteLine);
}

<h2>@ViewBag.Title</h2>

<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Id)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Name)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.UserName)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Email)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Age)</th>
        </tr>
    </thead>
    <tbody>
        @{int i = 0;}
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@Html.DisplayFor(modelItem => item.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.Name)</td>
                <td>@Html.DisplayFor(modelItem => item.UserName)</td>
                <td>@Html.DisplayFor(modelItem => item.Email)</td>
                <td>@Html.DisplayFor(modelItem => item.Age)</td>
            </tr>
            i++;
        }
    </tbody>
</table>